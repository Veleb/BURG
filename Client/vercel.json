{
  "version": 2,
  "framework": "angular",
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "outputDirectory": "dist/browser",
  "trailingSlash": true,
  "rewrites": [
    { "source": "/(.*)", "destination": "/dist/client/browser/index.html/index.html" }
  ],
  "redirects": [
    { "source": "/(.*)", "destination": "/", "statusCode": 308 }
  ]
}
